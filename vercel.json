{
  "redirects": [
    {
      "source": "/resources/mainnet-releases",
      "destination": "/resources/mainnet"
    },
    {
      "source": "/resources/testnet-releases",
      "destination": "/resources/testnet"
    },
    {
      "source": "/releases(.*)",
      "destination": "/resources$1"
    },
    {
      "source": "/roles(.*)",
      "destination": "$1"
    },
    {
      "source": "/user(.*)",
      "destination": "/resources$1"
    }
  ]
}
